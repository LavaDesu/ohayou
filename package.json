{
    "name": "ohayou",
    "version": "0.3.0",
    "description": "A node.js wrapper for the upcoming osu!api v2",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "tsc",
        "build:check": "tsc --noEmit",
        "docs": "typedoc src/index.ts --excludePrivate --out docs --mode library --disableSources --tsconfig tsconfig.doc.json --readme ./README.md --toc Enums,Base,Client,Instance,RequestHandler,Score,User",
        "lint": "eslint -c .eslintrc.json --ext .ts src"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LavaDesu/ohayou.git"
    },
    "keywords": [
        "osu",
        "api",
        "wrapper",
        "oauth2"
    ],
    "author": "LavaDesu <me@lava.moe> (https://lava.moe)",
    "files": [
        "lib"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/LavaDesu/ohayou/issues"
    },
    "homepage": "https://github.com/LavaDesu/ohayou#readme",
    "devDependencies": {
        "@types/node": "^14.14.19",
        "@typescript-eslint/eslint-plugin": "^4.11.1",
        "@typescript-eslint/parser": "^4.11.1",
        "eslint": "^7.17.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-unicorn": "^25.0.1",
        "typedoc": "^0.20.9",
        "typescript": "^4.1.3"
    },
    "dependencies": {}
}
