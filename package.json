{
  "name": "ohayou",
  "version": "0.1.0",
  "description": "A node.js wrapper for the upcoming osu!api v2",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "docs": "typedoc src/index.ts --excludePrivate --out docs --mode library --tsconfig tsconfig.doc.json",
    "test": "mocha test/**.ts --timeout 20000 -r ts-node/register",
    "start": "tsc && node lib/index.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoeLava/ohayou.git"
  },
  "keywords": [
    "osu",
    "api",
    "wrapper",
    "oauth2"
  ],
  "author": "MoeLava <me@lava.moe> (https://lava.moe)",
  "files": [
    "lib"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MoeLava/ohayou/issues"
  },
  "homepage": "https://github.com/MoeLava/ohayou#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.1",
    "chai": "^4.2.0",
    "mocha": "^7.1.0",
    "ts-node": "^8.6.2",
    "tslint": "^6.1.0",
    "typedoc": "^0.17.0-3",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}
