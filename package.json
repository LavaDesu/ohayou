{
  "name": "ohayou",
  "version": "0.2.0",
  "description": "A node.js wrapper for the upcoming osu!api v2",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "docs": "typedoc src/index.ts --excludePrivate --out docs --mode library --theme minimal --tsconfig tsconfig.doc.json",
    "lint": "eslint -c .eslintrc.json --ext .ts src",
    "test": "mocha test/**.ts --timeout 20000 -r ts-node/register",
    "start": "tsc && node lib/index.js"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoeLava/ohayou.git"
  },
  "keywords": [
    "osu",
    "api",
    "wrapper",
    "oauth2"
  ],
  "author": "MoeLava <me@lava.moe> (https://lava.moe)",
  "files": [
    "lib"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MoeLava/ohayou/issues"
  },
  "homepage": "https://github.com/MoeLava/ohayou#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.4",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-unicorn": "^19.0.1",
    "mocha": "^7.1.1",
    "ts-node": "^8.8.1",
    "typedoc": "^0.17.3",
    "typescript": "^3.9.3"
  },
  "dependencies": {}
}
